{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "src", 
    "admin/pages/register/components/AppointmentsManagement.tsx", 
    "admin/pages/register/components/Dashboard-Admin.tsx", 
    "admin/pages/register/components/Appointments-Admin.tsx", 
    "admin/pages/register/components/GuidanceServices.tsx", 
    "admin/pages/register/components/Index-Admin.tsx", 
    "admin/pages/register/components/Index-Frontdesk.tsx", 
    "admin/pages/register/components/App.tsx", 
    "admin/pages/register/components/Logs.tsx", 
    "admin/pages/register/components/Login-Admin.tsx", 
    "admin/pages/register/components/Appointments.tsx", 
    "admin/pages/register/components/Dashboard.tsx", 
    "admin/pages/register/components/ExcuseSlip.tsx", 
    "admin/pages/register/components/Frontdesk.tsx", 
    "admin/pages/register/components/StudentRecords.tsx", 
    "admin/pages/register/components/ViewSchedules.tsx", 
    "admin/pages/register/components/Reports-Admin.tsx", 
    "admin/pages/register/components/Register-Admin.tsx", 
    "admin/pages/register/components/Reports.tsx", 
    "admin/pages/register/components/ReviewExcuses.tsx"],
  "exclude": ["node_modules", "dist"]
}
